<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appsmarthome">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppSmartHome">
        <activity
            android:name=".KitchenLineChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity"/>
        <activity
            android:name=".LivingRoomLineChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity"/>
        <activity
            android:name=".BedRoomLineChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity"/>
        <activity
            android:name=".KitchenChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity" />
        <activity
            android:name=".BedRoomChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity" />
        <activity
            android:name=".LivingRoomChart"
            android:exported="false"
            android:parentActivityName=".RemoteActivity" />
        <activity
            android:name=".UserActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RemoteActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>